from split_evens_odds import SplitEvensOdds

lst = SplitEvensOdds()
lst.build_forward_list([1,2,3,4,5,6,7,8,15,14,13,12,11,10,9])

print("Original List:")
lst.display()

evens, odds = lst.split()

print("\nEvens:")
evens.display()

print("\nOdds:")
odds.display()

print("\nOriginal List After Split:")
lst.display()
